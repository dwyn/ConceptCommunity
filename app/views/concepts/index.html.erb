<div class = "favorite_concepts">
  <h3> Favorite Concepts! </h3>
  <% @favorites.each do |concept| %>
    <%= link_to "#{concept.title}", user_concept_path(concept.user_id, concept.id) %><br>
  <% end %>
</div>

<div class = "remainingConcepts">
  <%= link_to "Remaining Concepts", user_concepts_path(@user, @concept), class: "col-2 center btn btn-info remainingConcepts"  %>
</div>


